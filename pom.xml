<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <parent>
    <groupId>org.jenkins-ci.plugins</groupId>
    <artifactId>plugin</artifactId>
    <version>3.5</version>
  </parent>

  <artifactId>script-watcher-plugin</artifactId>
  <version>1.0-SNAPSHOT</version>
  <packaging>hpi</packaging>
  <name>Script Watcher Plugin</name>
  <description>This plugin run some scripts whenever node online.</description>
  <url>https://github.com/mamh-java/mail-watcher-plugin.git</url>

  <scm>
    <connection>scm:git:https://github.com/mamh-java/mail-watcher-plugin.git</connection>
    <developerConnection>scm:git:https://github.com/mamh-java/mail-watcher-plugin.git</developerConnection>
    <url>https://github.com/mamh-java/mail-watcher-plugin.git</url>
    <tag>HEAD</tag>
  </scm>

  <repositories>
    <repository>
      <id>repo.jenkins-ci.org</id>
      <url>https://repo.jenkins-ci.org/public/</url>
    </repository>
  </repositories>

  <pluginRepositories>
    <pluginRepository>
      <id>repo.jenkins-ci.org</id>
      <url>https://repo.jenkins-ci.org/public/</url>
    </pluginRepository>
  </pluginRepositories>

  <properties>
    <jenkins.version>2.222.3</jenkins.version>
    <slf4jVersion>1.7.26</slf4jVersion>
    <java.level>8</java.level>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    <surefire.useFile>false</surefire.useFile>
  </properties>

  <developers>
    <developer>
      <id>bright</id>
